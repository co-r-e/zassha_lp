# Required for accessing Gemini Files API
GEMINI_API_KEY=

# Optional: segment length (seconds) when ffmpeg is available
ZASSHA_SEGMENT_LEN=

# Set when deploying under a subpath (e.g. GitHub Pages project site /repo-name)
NEXT_PUBLIC_BASE_PATH=
# Site URL used for generating absolute OGP links (e.g. https://username.github.io)
NEXT_PUBLIC_SITE_URL=
